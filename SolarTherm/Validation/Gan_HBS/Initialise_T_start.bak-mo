within SolarTherm.Validation.Gan_HBS;

function Initialise_T_start
  input Real z_start_data[:];
  input Real T_data[:];
  input Real z_array[100];
  output Real T_array[100];
algorithm
  for i in 1:100 loop
    T_array[i] := Modelica.Math.Vectors.interpolate(z_start_data,T_data,z_array[i]);
  end for;
annotation(
    Diagram(coordinateSystem(preserveAspectRatio = false)));
end Initialise_T_start;